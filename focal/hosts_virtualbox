virtualbox ansible_host=127.0.0.1 ansible_port=2222

[default]
virtualbox

[default:vars]
ansible_python_interpreter=/usr/bin/python3
# last variable from here: https://www.toptechskills.com/ansible-tutorials-courses/how-to-fix-usr-bin-python-not-found-error-tutorial/
# Won't be need when the host is ubuntu 20.04 or greater, which uses python3 by default.
